# Example environment file - copy to .env and fill in real values
# Never commit real credentials to version control

# GoCardless Access Token (primary payment integration)
GOCARDLESS_ACCESS_TOKEN=your_gocardless_access_token_here

# TrueLayer API Credentials (if needed for bank connectivity)
TRUELAYER_CLIENT_ID=your_client_id_here
TRUELAYER_CLIENT_SECRET=your_client_secret_here

# Firebase Configuration (optional - fallbacks exist)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_APP_ID=your_app_id

# OpenAI API Key (for AI features)
OPENAI_API_KEY=your_openai_api_key

# Brandfetch API Key (for merchant logos)
BRANDFETCH_API_KEY=your_brandfetch_api_key